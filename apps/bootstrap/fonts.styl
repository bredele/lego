@font-face {
	font-family: 'maple';
	src:url('./fonts/maple.eot');
	src:url('./fonts/maple.eot?#iefix') format('embedded-opentype'),
		url('./fonts/maple.woff') format('woff'),
		url('./fonts/maple.ttf') format('truetype'),
		url('./fonts/maple.svg#maple') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'maple';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-down:before {
	content: "\e600";
}
.icon-cross:before {
	content: "\e601";
}
.icon-list:before {
	content: "\e602";
}