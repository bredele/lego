{
  "name": "brickjs",
  "version": "0.9.0",
  "description": "MVVM building block to create real time and large scale applications in a flash",
  "main": "index.js",
  "scripts": {
    "test": "zuul -- test/*.js",
    "test-local": "zuul --local -- test/*.js",
    "standalone": "component build --standalone brick --name brick --out .",
    "test-server": "mocha test/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bredele/brick.git"
  },
  "keywords": [
    "MVVM",
    "brick",
    "cement",
    "wall",
    "store",
    "model",
    "binding"
  ],
  "author": "Olivier Wietrich <olivier.wietrich@gmail.com> (http://github.com/bredele)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bredele/brick/issues"
  },
  "dependencies": {
    "cement": "git://github.com/bredele/cement",
    "datastore": "git://github.com/bredele/datastore",
    "fragment": "git://github.com/bredele/fragment",
    "grout": "git://github.com/bredele/grout",
    "many": "git://github.com/bredele/many",
    "mouth": "git://github.com/bredele/mouth",
    "stomach": "git://github.com/bredele/stomach"
  },
  "devDependencies": {
    "mocha": "^2.2.5",
    "roof": "git://github.com/bredele/roof",
    "zuul": "~1.5.4"
  }
}
